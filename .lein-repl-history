(def db-uri
  (java.net.URI. (or
    (System/getenv "DATABASE_URL")
    "postgresql://localhost:5432/ticks")))
(println db-uri)
exit
(require '[clojure.java.jdbc :as sql])
(def db-spec {:classname "org.postgresql.Driver" :subprotocol "postgresql" :subname "postgresql://localhost:5432/weatherman" :user "weatherman" :password "rainman"})
(sql/insert! db-spec :testing {:data "Hello World"})
(def db-spec {:classname "org.postgresql.Driver" :subprotocol "postgresql" :subname "//localhost:5432/weatherman" :user "weatherman" :password "rainman"})
(sql/insert! db-spec :testing {:data "Hello World"})
(sql/create-table-ddl db-spec :testing [:data :text])
(sql/db-do-commands db-spec (sql/create-table-ddl :testing [:data :text]))
(sql/insert! db-spec :testing {:data "Hello World"})
(sql/query db-spec ["select * from testing"])
(sql sb-do-commands db-spec "drop table testing")
(sql/db-do-commands db-spec "drop table testing")
exit
